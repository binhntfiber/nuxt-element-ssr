{
  "name": "nuxt3-awesome-starter",
  "version": "0.1.0",
  "description": "a Nuxt 3 starter template or boilerplate with a lot of useful features. Nuxt3 + Tailwindcss 3",
  "author": "fiberblock",
  "license": "MIT",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "serve": "serve dist/",
    "postinstall": "husky install",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "lintfix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "prepare": "husky install",
    "clean": "rm -rf .nuxt dist .output",
    "generate:locales": "node tools/translator.js ./locales en.yml"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@element-plus/nuxt": "^1.0.3",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@headlessui/vue": "^1.7.4",
    "@iconify/json": "^2.1.135",
    "@intlify/nuxt3": "^0.2.4",
    "@nuxt/test-utils-edge": "3.0.0-rc.13-27772354.a0a59e2",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@pinia/nuxt": "^0.4.3",
    "@types/cleave.js": "^1.4.7",
    "@vueuse/core": "^9.6.0",
    "@vueuse/nuxt": "^9.6.0",
    "browserify-zlib": "^0.2.0",
    "element-plus": "^2.2.32",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "js-yaml": "^4.1.0",
    "lint-staged": "^13.0.3",
    "nuxt": "^3.2.2",
    "nuxt-windicss": "^2.6.0",
    "pinia": "^2.0.23",
    "postcss": "8.4.14",
    "postcss-loader": "^7.0.0",
    "prettier": "^2.7.1",
    "sass": "1.53.0",
    "sass-loader": "^13.2.0",
    "serve": "^13.0.2",
    "stream-browserify": "^3.0.0",
    "three": "^0.143.0",
    "translate": "^1.4.1",
    "typescript": "^4.8.4",
    "unplugin-icons": "^0.14.14",
    "unplugin-vue-components": "^0.22.9",
    "vitest": "^0.23.4",
    "vue": "^3.2.41",
    "vue-tsc": "^1.0.9",
    "webpack": ">=5.0.0 <6.0.0"
  },
  "lint-staged": {
    "**/*.{js,ts,vue,html}": [
      "yarn lintfix"
    ]
  },
  "engines": {
    "node": ">=16 <17"
  },
  "dependencies": {
    "@types/numeral": "^2.0.2",
    "bignumber.js": "^9.1.1",
    "cleave.js": "^1.6.0",
    "dayjs": "^1.11.7",
    "ethers": "^5.7.2",
    "lodash-es": "^4.17.21",
    "numeral": "^2.0.6",
    "qs": "^6.11.0",
    "vue-toastification": "^2.0.0-rc.5",
    "vue-web3-wallets": "^1.1.3"
  }
}
